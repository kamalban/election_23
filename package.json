{
  "name": "election-ke-baazigar",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "build-locale:en": "ng build --prod --i18n-locale en --i18n-format xlf --i18n-file src/translate/messages.en.xlf --output-path=dist/en --baseHref /en/",
    "build-locale:hi": " ng build --prod --i18n-locale hi --i18n-format xlf --i18n-file src/translate/messages.hi.xlf --output-path=dist/hi --baseHref /hi/",
    "build-locale:pb": " ng build --prod --i18n-locale pb --i18n-format xlf --i18n-file src/translate/messages.pb.xlf --output-path=dist/pb --baseHref /pb/",
    "build-locale": "npm run build-locale:en && npm run build-locale:hi",
    "i18n:extract": "ng xi18n --output-path src/locale"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.1.0",
    "@angular/common": "~13.1.0",
    "@angular/compiler": "~13.1.0",
    "@angular/core": "~13.1.0",
    "@angular/forms": "~13.1.0",
    "@angular/localize": "^13.4.0",
    "@angular/platform-browser": "~13.1.0",
    "@angular/platform-browser-dynamic": "~13.1.0",
    "@angular/router": "~13.1.0",
    "@ng-bootstrap/ng-bootstrap": "^10.0.0",
    "@ngneat/transloco": "^3.1.1",
    "@ngx-translate/core": "^14.0.0",
    "@ngx-translate/http-loader": "^7.0.0",
    "apexcharts": "^3.33.0",
    "bootstrap": "^5.1.3",
    "canvas-confetti": "^1.4.0",
    "jquery": "^3.6.0",
    "localize": "^0.4.7",
    "ng-apexcharts": "^1.6.0",
    "ng2-charts": "^2.3.0",
    "ngx-bootstrap-slider": "^1.9.0",
    "ngx-toastr": "^14.2.1",
    "rxjs": "~7.4.0",
    "swiper": "^7.4.1",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.1.1",
    "@angular/cli": "~13.1.1",
    "@angular/compiler-cli": "~13.1.0",
    "@types/canvas-confetti": "^1.4.2",
    "@types/jasmine": "~3.10.0",
    "@types/jquery": "^3.5.11",
    "@types/node": "^12.11.1",
    "jasmine-core": "~3.10.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.1.0",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "typescript": "~4.5.2"
  }
}
